{
  "@@locale": "en",
  "helloWorld": "Hello World!",
  "changeLanguage": "Change language",

  "notificationScreen": "Notification Screen",
  "@notificationScreen": {
    "description": "Screen for testing notification"
  },
  "enterYourNameHere": "Enter your name here",
  "@enterYourNameHere": {
    "description": "Ask user to enter their name"
  },
  "yourNameIs": "Your name is {userName}",
  "@yourNameIs": {
    "description": "Show username",
    "placeholders": {
      "userName": {
        "type": "String",
        "example": "Huy"
      }
    }
  },
  "enterAnyNumberHere": "Enter any number here",
  "@enterAnyNumberHere": {
    "description": "Ask user to enter any number"
  },
  "userNumberIs": "{username}'s number is {number}",
  "@userNumberIs": {
    "description": "Show user's number",
    "placeholders": {
      "username": {
        "type": "String",
        "example": "Huy"
      },
      "number": {
        "type": "double",
        "example": "3.14",
        "format": "compactSimpleCurrency",
        "optionalParameters": {
          "decimalDigits": 1
        }
      }
    }
  },
  "todayTomorrow": "Today is {today}, tomorrow is {tomorrow}",
  "@todayTomorrow": {
    "description": "Show today and tomorrow for user",
    "placeholders": {
      "today": {
        "type": "DateTime",
        "format": "yMd"
      },
      "tomorrow": {
        "type": "DateTime",
        "format": "yMd"
      }
    }
  },
  "nThings": "{count,plural, =0{no {thing}} =1{{count} {thing}} other{{count} {thing}s}",
  "@nThings": {
    "description": "plural message with thing parameter",
    "placeholders": {
      "count": {
        "type": "int"
      },
      "thing": {
        "type": "String",
        "example": "chicken"
      }
    }
  },
  "chicken": "chicken",
  "@chicken":{
    "description": "Chicken in english"
  }
}